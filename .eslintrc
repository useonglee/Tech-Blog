{
  "parser": "babel-eslint",
  "parserOptions": {
    "project": "./jsconfig.json",
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@eslint/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["import", "react-hooks", "eslint"],
  "rules": {
    "eslint/explicit-module-boundary-types": "off",
    "eslint/no-var-requires": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
